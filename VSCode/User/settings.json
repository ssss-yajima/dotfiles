{
  //========== VSNote =============
  "vsnotes.defaultNotePath": "/Users/syajima/develop/MyActivities/docs",
  "vsnotes.defaultNoteTitle": "{title}.{ext}",
  "vsnotes.listRecentLimit": 30,
  "vsnotes.templates": ["mydefault", "seminar", "book", "blog"],
  "vsnotes.defaultSnippet": {
    "langId": "markdown",
    "name": "vsnote_template_mydefault"
  },
  // ===============
  "explorer.confirmDragAndDrop": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "git.enableSmartCommit": true,
  "editor.detectIndentation": true,
  "[python]": {
    "editor.tabSize": 4
  },
  // Controls if the editor will insert spaces for tabs. Accepted values:  "auto", true, false. If set to "auto", the value will be guessed when a file is opened.
  "editor.insertSpaces": true,
  // Controls whether the editor should render whitespace characters
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "editor.fontSize": 16,
  "debug.console.fontSize": 16,
  "terminal.integrated.fontSize": 16,
  "python.pythonPath": "/usr/local/bin/python3",
  "markdown-preview-enhanced.breakOnSingleNewLine": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "python.formatting.provider": "yapf",
  "editor.fontFamily": "SourceHanCodeJP-Regular, Menlo, Monaco, 'Courier New', monospace",
  "vsicons.dontShowNewVersionMessage": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": -1,
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"/Users/syajima/.vscode/extensions/gabrielbb.vscode-lombok-1.0.1/server/lombok.jar\"",
  "java.configuration.checkProjectSettingsExclusions": false,
  "java.saveActions.organizeImports": true,
  "gradle.debug": true,
  "boot-java.change-detection.on": true,
  "explorer.confirmDelete": false,
  "markdown.preview.fontSize": 16,
  "java.semanticHighlighting.enabled": true,
  "java.format.enabled": false,
  "html.format.preserveNewLines": false,
  "hediet.vscode-drawio.local-storage": "",
  "eslint.packageManager": "yarn",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "stylelint.autoFixOnSave": true,
  "stylelint.configOverrides": {
    "ignoreFiles": ["node_modules/**/*.*"]
  }
}
