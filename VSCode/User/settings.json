{
	// VSNote
	"vsnotes.defaultNotePath": "/Users/syajima/develop/MyActivities/docs",
	"vsnotes.defaultNoteTitle": "{title}.{ext}",
	"vsnotes.listRecentLimit": 30,
	"vsnotes.templates": [
		"mydefault",
		"seminar",
		"book",
		"blog",
	],
	"vsnotes.defaultSnippet": {
		"langId": "markdown",
		"name": "vsnote_template_mydefault"
	},
	"python.jediEnabled": false,
	"explorer.confirmDragAndDrop": false,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"git.enableSmartCommit": true,
	"editor.detectIndentation": true,

	"files.associations": {
		"*.vue": "vue"
	},
	"[python]": {
		"editor.tabSize": 4
	},
	"eslint.validate": [
		{
			"language": "vue",
			"autoFix": true
		},
		{
			"language": "javascript",
			"autoFix": true
		}
	],
	"eslint.autoFixOnSave": true,

	// Controls if the editor will insert spaces for tabs. Accepted values:  "auto", true, false. If set to "auto", the value will be guessed when a file is opened.
	"editor.insertSpaces": true,
	// Controls whether the editor should render whitespace characters
	"editor.renderWhitespace": "all",

	"vetur.format.defaultFormatter.js": "prettier-eslint",
	"editor.formatOnPaste": true,
	"editor.tabSize": 2,
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"editor.fontSize": 14,
	"debug.console.fontSize": 14,
	"terminal.integrated.fontSize": 14,
	"python.pythonPath": "/usr/local/bin/python3",
	"markdown-preview-enhanced.breakOnSingleNewLine": false,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"python.formatting.provider": "yapf",
	"editor.fontFamily": "SourceHanCodeJP-Regular, Menlo, Monaco, 'Courier New', monospace",
	"vsicons.dontShowNewVersionMessage": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"window.zoomLevel": -1,
}