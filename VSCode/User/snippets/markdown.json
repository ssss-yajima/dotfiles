{
	// Place your snippets for markdown here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

"vsnote_template_mydefault": {
	"prefix": "vsnote_template_mydefault",
	"body": [
		"---",
		"title: $TM_FILENAME_BASE",
		"date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
		"tags:",
		"  - $2",
		"---",
		"$3",
	],
	"description": "Default template",
},
"vsnote_template_blog": {
	"prefix": "vsnote_template_blog",
	"body": [
		"---",
		"title: $TM_FILENAME_BASE",
		"date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
		"tags:",
		"  - blog",
		"  - WIP",
		"  - $2",
		"---",
		"$3",
	],
	"description": "Template for blog",
},
	"vsnote_template_seminar": {
		"prefix": "vsnote_template_seminar",
		"body": [
			"---",
			"title: $TM_FILENAME_BASE",
			"date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
			"tags:",
			"  - seminar",
			"  - $2",
			"---",
			"# $TM_FILENAME_BASE",
			"場所：$3",
			"",
			"# 概要",
			"$4",
			"# ",
		],
		"description": "勉強会記録のテンプレート",
	},
"vsnote_template_book": {
	"prefix": "vsnote_template_book",
	"body": [
			"---",
			"title: $TM_FILENAME_BASE",
			"date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
			"tags:",
			"  - book",
			"  - $2",
			"---",

			"# $TM_FILENAME_BASE",
			"著者：$3",
			"出版社：$4",
			"版：$5",
			"",
			"# 書籍の概要",
			"$6",
			"# こんな方にオススメ",
			"",
			"# 要約・感想",
		],
		"description": "読書記録のテンプレート",
	},
}